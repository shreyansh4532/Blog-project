<%- include('partials/header') -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post" autocomplete="off">

  <div class="mb-3">
    <label for="formGroupExampleInput" class="form-label">Title</label>
    <input type="text" class="form-control" id="formGroupExampleInput" name="postTitle">
    <label for="postBox">Post</label><br>
    <textarea class="form-control" id="postBox" name="postBody" rows="8" cols="80"></textarea><br>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </div>


</form>

